
<div class="form padded text-center">
  <div class="col-sm-4 col-md-offset-4">
    <%= simple_form_for @service, url: url do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :city %>
    <%= f.input :price %>


    <p>
      <%= f.button :submit, class: "btn btn-info" %>
    </p>
    <% end %>

  </div>
</div>
